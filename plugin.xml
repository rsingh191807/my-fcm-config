
<?xml version="1.0" encoding="utf-8"?>
<plugin id="fcm.config" version="1.0.0"
        xmlns="http://apache.org/cordova/ns/plugins"
        xmlns:android="http://schemas.android.com/apk/res/android">

  <name>FCMConfig</name>
  <description>Places google-services.json inside the Android app module</description>
  <license>MIT</license>

  <platform name="android">
    <resource-file src="src/android/google-services.json" target="app/google-services.json" />
  </platform>

</plugin>
